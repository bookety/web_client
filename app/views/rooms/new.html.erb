<h1><%= @place.fantasy_name %></h1>
<%= simple_form_for [@place, @room] do |f| %>
    <div class="form-inputs">
        <%= f.input :room_name %>
        <%= f.input :room_number %>
    </div>
    <div class="form-actions">
        <%= f.submit  %>
    </div>
<% end %>

<% unless @rooms.nil? %>
    <% @rooms.each do |room| %>
        <p><%= room.room_name %></p>
    <% end %>
<% end %>
